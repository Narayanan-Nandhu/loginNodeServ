{
  "name": "loginnodeserv",
  "version": "1.0.1",
  "description": "Login node module service layer to handle Passport authentication",
  "main": "dist",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=\"test\" tsc && jest --coverage",
    "test:watch": "jest --watch",
    "transpile": "tsc --watch",
    "start": "nodemon node dist/index.js",
    "dev": "cross-env NODE_ENV=test PORT=5001 npm run start",
    "clean": "del ./*.js, ./*.js.map",
    "prepublishOnly": "\"npm run test\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Narayanan-Nandhu/loginNodeServ"
  },
  "keywords": [
    "Login",
    "Passport",
    "Authentication",
    "GAuth",
    "Social",
    "login",
    "local - Authentication",
    "service"
  ],
  "author": "Narayanan",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth": "^1.0.42",
    "@types/passport-google-oauth20": "^2.0.11",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "mongoose": "^6.3.2",
    "nodemon": "^2.0.16",
    "passport": "^0.5.2",
    "passport-google-oauth": "^2.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/jest": "^28.1.0",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.6",
    "dotenv": "^16.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1"
  }
}
